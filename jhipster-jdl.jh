entity Study {
	name String,
	description String,
	incentive String,
}

entity Researcher {
	email String,
    occupation String,
    institute String
}

entity Participant {
	email String,
    occupation String
}

relationship ManyToMany {
	Study{participant(email)} to Participant{study}
}

relationship OneToMany {
	Study{researcher(email)} to Researcher{study}
}